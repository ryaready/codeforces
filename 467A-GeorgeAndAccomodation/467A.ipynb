    "n = int(input())\n",
    "\n",
    "count = 0\n",
    "\n",
    "for i in range(n):\n",
    "    ls = list(map(int, input().rstrip().split()))\n",
    "    a = ls[0]\n",
    "    b = ls[1]\n",
    "\n",
    "    if a + 1 < b: \n",
    "        count += 1\n",
    "\n",
    "print(count)"
